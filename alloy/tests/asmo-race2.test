// Test that threads 3 and 4 can't read updates to x in conflicting order
NEWWG
NEWSG
NEWTHREAD
st.atom.rel.scopedev.sc0.semsc0.semav x = 1
NEWWG
NEWSG
NEWTHREAD
st.atom.rel.scopedev.sc0.semsc0.semav x = 2
NEWWG
NEWSG
NEWTHREAD
ld.atom.acq.scopedev.sc0.semsc0.semvis x = 1
ld.atom.acq.scopedev.sc0.semsc0.semvis x = 2
NEWWG
NEWSG
NEWTHREAD
ld.atom.acq.scopedev.sc0.semsc0.semvis x = 2
ld.vis.scopedev.sc0 x = 2
SATISFIABLE consistent[X] && #dr>0
NOSOLUTION consistent[X] && #dr=0